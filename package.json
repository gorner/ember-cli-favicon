{
  "name": "@gorner/ember-cli-favicon",
  "version": "4.0.0-alpha.2",
  "description": "Automatically build and include all the various platform / device favicon formats from a single source public/favicon.png.",
  "keywords": [
    "ember-addon",
    "favicon"
  ],
  "repository": "gorner/ember-cli-favicon",
  "license": "MIT",
  "contributors": [
    "Dave Wasmer"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "mocha tests/build.js",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "release-it"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "@gorner/broccoli-favicon": "^4.0.0-alpha.1",
    "broccoli-merge-trees": "^4.2.0",
    "ember-cli-babel": "^7.26.6 || ^8.0.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "^2.5.0",
    "@embroider/test-setup": "^0.47.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "^2.2.3",
    "ember-cli": "~3.28.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^6.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.10.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "md5-file": "^5.0.0",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "release-it": "^16.2.0",
    "webpack": "^5.58.2"
  },
  "overrides": {
    "ansi-html": "^0.0.9"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
